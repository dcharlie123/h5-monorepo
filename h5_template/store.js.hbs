import { defineStore } from 'pinia';
export const useCountStore = defineStore('count',{
    state: () => ({
        counter: 1
    }),
    getters: {
        doubleCount: (state) => state.counter*2
    },
    actions: {
        add() {
            this.counter++
        },
        minnus() {
            this.counter--;
        }
    }
})